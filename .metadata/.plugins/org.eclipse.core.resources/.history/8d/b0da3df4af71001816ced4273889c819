#include "Types.h"
#include "utilss.h"
#include "ADC.h"
#include "DIO.h"

void ADC_voidInit (void)
{
	ADMUX=0b11100000;	/* ADC Multiplexer Selection Register */
	/*		||||||||
	 * 		|||||||-- MUX0 {Select ADC0 Channel}
	 * 		||||||--- MUX1
	 * 		|||||---- MUX2
	 * 		||||----- MUX3
	 * 		|||------ MUX4
	 * 		||------- ADLAR {Left adjust}
	 * 		|-------- REFS0 {Internal 2.56V Voltage Reference}
	 * 		--------- REFS1
	 * */

}
